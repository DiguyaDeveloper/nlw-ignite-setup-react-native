{
  "name": "nlw-ignite-setup-react-native",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier  \"src/**/*.+(ts|js|html|json)\" --check",
    "prettier:fix": "prettier --write \"src/**/*.+(ts|js|html|json)\"",
    "migration:dev": "npx prisma migrate dev",
